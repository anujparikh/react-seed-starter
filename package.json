{
  "name": "react-seed-starter",
  "version": "0.0.1",
  "description": "This is seed starter for react project",
  "scripts": {
    "prestart": "babel-node setup/start-message.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node setup/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src setup",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress setup/test-setup.js \"test/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
